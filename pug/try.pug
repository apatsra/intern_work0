doctype html
html
    include includes/main.pug
    head
    body
    mixin try(title)
      #staticBackdrop.modal.fade(data-bs-backdrop='static' data-bs-keyboard='false' tabindex='-1' aria-labelledby='staticBackdropLabel' aria-hidden='true')
        .modal-dialog
          .modal-content
            .modal-header.btn.btn-warning
              h5.text-primary.modal-title Let's park
              button.btn-close(type='button' data-bs-dismiss='modal' aria-label='Close')
            .modal-body.btn.btn-danger
                h1= title           
            .modal-footer
              button.btn.btn-danger(type='button' data-bs-dismiss='modal') Close
              a(href='checkout.html')
                button.btn.btn-success(type='button') Yes
    
    each val in ['B01', 'B02', 'B03', 'B04', 'B05','B06']
      .col-12.col-sm-4.col-md-3.col-lg-4.col-xl-2
        .item.text-light.fw-bold.px-3.box-B.cover#item= val
          .text-light.fw-normal.fs-0 $9
          button.book-button.text-light.box-B.button(type='button' data-bs-toggle='modal' data-bs-target='#staticBackdrop') Book  Now
            +try('B')
                                                             
    each val in ['C01', 'C02', 'C03', 'C04', 'C05','C06']
      .col-12.col-sm-4.col-md-3.col-lg-4.col-xl-2
        .item.text-light.fw-bold.px-3.box-C.cover#item= val
          .text-light.fw-normal.fs-0 $9
          button.book-button.text-light.box-C.button(type='button' data-bs-toggle='modal' data-bs-target='#staticBackdrop') Book  Now
            +try('C')
             

              
              
          